def leap(year):
    if year%4==0 and year%100!=0:
       print("Yes")
    else:
       print("No")
year=int(input())
leap(year)



def leap(year):
    if year%4==0 and year%100!=0:
       return "Yes"
    else:
       return "No"
year=int(input())
print(leap(year))



def leap(year):
    if year%4==0 and year%100!=0:
       return True
    elif year%400==0:
       return True
    else:
       return False
year=int(input())
if leap(year):
   print("Yes")
else:
   print("No)
